<h1><%= @event.title %></h1>

<div class="container">

  <div class="col-md-4 col-md-offset-2">
    <h2>Participants</h2>

    <ul>
      <% for p in @event.participants %>
        <li>
          <%= p.name %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-4">
    <h2>Constraints</h2>

    No constraints.
  </div>

</div>

<%= link_to 'Edit this event', edit_event_path(@event) %>
